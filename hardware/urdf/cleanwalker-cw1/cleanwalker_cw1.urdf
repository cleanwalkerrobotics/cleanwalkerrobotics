  <!-- ================================================================ -->
  <!--                     BAG SYSTEM                                   -->
  <!-- ================================================================ -->

  <!-- Bag roll mount: fixed dispenser cylinder on top-rear of body -->
  <joint name="bag_roll_mount_joint" type="fixed">
    <parent link="body"/>
    <child link="bag_roll_mount"/>
    <origin xyz="-0.20 0 0.06" rpy="0 0 0"/>
