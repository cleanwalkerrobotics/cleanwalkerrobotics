  <!--                     FRONT-LEFT LEG (FL)                          -->
  <!-- ================================================================ -->

  <!-- FL hip yaw: rotates leg left/right -->
  <joint name="FL_hip_yaw_joint" type="revolute">
    <parent link="body"/>
