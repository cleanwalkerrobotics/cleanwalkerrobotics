<?xml version="1.0" encoding="utf-8"?>
<!--
  CleanWalker CW-1 — URDF Robot Description
  Version: 1.0
  Date: 2026-02-11

  18 actuated DOF:
    - 4 legs × 3 DOF (hip_yaw, hip_pitch, knee_pitch) = 12
    - 1 arm × 5 DOF (turret_yaw, shoulder_pitch, elbow_pitch, wrist_pitch, gripper) = 5
    - 1 bag frame hinge = 1

  Total mass: ~15 kg
  Body: 0.60 × 0.15 × 0.12 m
  Leg length: ~0.40 m (0.20 upper + 0.20 lower)
  Arm reach: ~0.46 m (0.18 upper + 0.18 forearm + 0.10 gripper)

  Coordinate convention:
    X = forward, Y = left, Z = up
-->
<robot name="cleanwalker_cw1">

  <!-- ========== MATERIALS ========== -->

  <material name="olive_green">
    <color rgba="0.231 0.290 0.247 1.0"/>
  </material>
  <material name="dark_grey">
    <color rgba="0.20 0.20 0.20 1.0"/>
  </material>
  <material name="black">
    <color rgba="0.10 0.10 0.10 1.0"/>
  </material>
  <material name="green_led">
    <color rgba="0.133 0.773 0.369 1.0"/>
  </material>

  <!-- ========== BASE (dummy root link) ========== -->

  <link name="base_link"/>

  <joint name="floating_base" type="fixed">
    <parent link="base_link"/>
    <child link="body"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <!-- ========== BODY ========== -->

  <link name="body">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="8.0"/>
      <inertia ixx="0.0246" ixy="0" ixz="0"
               iyy="0.2497" iyz="0"
               izz="0.2551"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.60 0.15 0.12"/>
      </geometry>
      <material name="olive_green"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.60 0.15 0.12"/>
      </geometry>
    </collision>
  </link>

  <!-- ========== HEAD (sensor module, flush with body front) ========== -->

  <joint name="head_joint" type="fixed">
    <parent link="body"/>
    <child link="head"/>
    <origin xyz="0.34 0 0.02" rpy="0 0 0"/>
  </joint>

  <link name="head">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.00121" ixy="0" ixz="0"
               iyy="0.000534" iyz="0"
               izz="0.00121"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.15 0.08"/>
      </geometry>
      <material name="olive_green"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.15 0.08"/>
      </geometry>
    </collision>
  </link>

