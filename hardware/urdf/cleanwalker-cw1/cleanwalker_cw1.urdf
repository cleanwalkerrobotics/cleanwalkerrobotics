    <inertial>
      <origin xyz="0 0 0.025" rpy="0 0 0"/>
      <mass value="0.15"/>
      <inertia ixx="0.0000397" ixy="0" ixz="0"
               iyy="0.0000397" iyz="0"
               izz="0.0000169"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.025" rpy="0 0 0"/>
      <geometry>
